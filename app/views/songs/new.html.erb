<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist %>
  <%= f.text_field :artist_name %>
  <br />
  <%= f.label :genre %>
  <%= f.text_field :genre_id, list: "genres_autocomplete" %>
  <datalist id = "genres_autocomplete">
    <% Genre.all.each do |genre| %>
      <option value = "<%= genre.name %>">
    <% end %>
  </datalist>
  <br />
  <input name = "song[note_contents][]" id = "song_notes_1">
  <br />
  <input name = "song[note_contents][]" id = "song_notes_2">
  <br />
  <%= f.submit %>
<% end %>


